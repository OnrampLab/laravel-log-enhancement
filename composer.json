{
    "name": "onramplab/laravel-log-enhancement",
    "description": "An enhanced logging package for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [],
    "authors": [
        {
            "name": "Kos Huang",
            "email": "kos.huang@onramplab.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "monolog/monolog": "^1.12|^2.0",
        "ramsey/uuid": "^4.0.0"
    },
    "require-dev": {
        "datadog/dd-trace": "^0.90",
        "fzaninotto/faker": "^1.9@dev",
        "illuminate/contracts": "^8.0",
        "illuminate/log": "^8.0",
        "illuminate/support": "^8.0",
        "mockery/mockery": "^1.3.2",
        "nunomaduro/larastan": "^1.0",
        "orchestra/database": "^6.0",
        "orchestra/testbench": "^6.0",
        "phpstan/phpstan": "^1.7",
        "phpunit/phpunit": "^8.0",
        "sempro/phpunit-pretty-print": "1.2.2",
        "symfony/thanks": "^1.0"
    },
    "suggest": {
        "datadog/dd-trace": "Datadog APM connect logs and traces."
    },
    "autoload": {
        "psr-4": {
            "Onramplab\\LaravelLogEnhancement\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Onramplab\\LaravelLogEnhancement\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "symfony/thanks": true
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Onramplab\\LaravelLogEnhancement\\LaravelLogEnhancementServiceProvider",
                "Onramplab\\LaravelLogEnhancement\\DatadogLoggingServiceProvider"
            ]
        }
    }
}
